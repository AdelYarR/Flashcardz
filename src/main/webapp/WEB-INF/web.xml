<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         version="2.5"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>UserRegistryServlet</servlet-name>
    <servlet-class>ru.itis.example.user.controllers.UserRegistryServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserRegistryServlet</servlet-name>
    <url-pattern>/registry</url-pattern>
  </servlet-mapping>


  <servlet>
  <servlet-name>UserLogServlet</servlet-name>
    <servlet-class>ru.itis.example.user.controllers.UserLogServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserLogServlet</servlet-name>
    <url-pattern>/log</url-pattern>
  </servlet-mapping>

</web-app>

